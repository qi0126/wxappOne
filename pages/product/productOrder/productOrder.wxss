/* pages/product/productOrder/productOrder.wxss */
.orderTitle{
  font-size:32rpx;
  font-weight:300;
  color:rgba(51,51,51,1);
  height: 131rpx;
  line-height: 131rpx;
  text-align: center;
}

.orderAddress{
  padding-left: 32rpx;
  padding-right: 38rpx;
  /* border: 1px solid red; */
  height: 381rpx;
  background-color: #fff;
}

.orderAddress .selectAdd{
  display: flex;
  font-size:28rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
  height: 119rpx;
  line-height:119rpx;
  border-bottom:1px solid rgba(228,228,228,1);
}

.orderAddress .take {
  margin-left: 110rpx;
}

.orderAddress .noSupport {
  margin-left: 17rpx;
  font-size:22rpx;
  font-weight:300;
  color:rgba(102,102,102,1);
}

.orderAddress .search_icon {
  vertical-align: middle;
}

.contactAddr {
  margin-top: 42rpx;
  position: relative;
}

.contactAddr .contacts {
  display: flex;
  margin-left: 109rpx;
  align-items: center;
}

.contactAddr .name {
  font-size:32rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
}
.contactAddr .tel {
  font-size:32rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
  margin: 0 20rpx;
}

.contactAddr .default{
  width:76rpx;
  height:28rpx;
  background:rgba(192,158,87,1);
  font-size:24rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(51,51,51,1);
  text-align: center;
  line-height: 28rpx;
}

.contactAddr .addrDetail {
  margin-top: 21rpx;
  margin-left: 109rpx;
  margin-right: 85rpx;
  font-size:26rpx;
  font-weight:300;
  color:rgba(102,102,102,1);
  line-height:49rpx;
}

.contactAddr .loactionIcon {
  width: 67rpx;
  height: 67rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.contactAddr .arrowIcon {
  width: 16rpx;
  height: 26rpx;
  position: absolute;
  top: 45rpx;
  right: 0;
}

.orderDetail {
  padding: 029rpx 38rpx 0 38rpx;
  background-color: #fff;
  height: 606rpx;
  margin-top: 26rpx;
}

.orderDetail .productPic {
  width: 139rpx;
  height: 168rpx;
}

.orderDetail .header{
  display: flex;
  align-items: center;
  border-bottom: 1px solid #E4E4E4;
  padding-bottom: 11rpx;
}

.orderDetail .picDes {
  margin-left: 23rpx;
}

.orderDetail .productDes {
  width: 415rpx;
  margin-left: 101rpx;
}

.orderDetail .productDes .detailDes {
  font-size:26rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
}

.orderDetail .productDes .format {
  font-size:24rpx;
  font-weight:300;
  color:rgba(153,153,153,1);
  display: flex;
  margin-top: 14rpx;
  margin-bottom: 34rpx;
} 

.orderDetail .productDes .size {
  margin-left: 50rpx;
}

.orderDetail .number {
  display: flex;
}

.orderDetail .number .price{
  font-size:32rpx;
  font-weight:normal;
  color:rgba(0,0,0,1);
}

.orderDetail .number .btnStyle{
  display: flex;
  margin-left: 53rpx;
  font-size:30rpx;
  font-weight:400;
  color:rgba(172,172,172,1);
  text-align: center;
  line-height: 53rpx;
}

.orderDetail .btnStyle .minu{
  width:59rpx;
  height:53rpx;
  border:1rpx solid rgba(192,192,192,1);
  border-top-left-radius: 14rpx;
  border-bottom-left-radius:14rpx;
}

.orderDetail .btnStyle .value{
  width:59rpx;
  height:53rpx;
  border:1rpx solid rgba(192,192,192,1);
  margin-left: 2rpx;
  margin-right: 3rpx;
  color: #474747;
}

.orderDetail .btnStyle .add{
  width:59rpx;
  height:53rpx;
  border:1rpx solid rgba(192,192,192,1);
  border-top-right-radius: 14rpx;
  border-bottom-right-radius:14rpx;
}

.orderDetail .middle{
  height: 98rpx;
  font-size:24rpx;
  font-weight:300;
  color:rgba(153,153,153,1);
  line-height: 98rpx;
  border-bottom: 1px solid #E4E4E4;
}

.orderDetail .footer {
  display: flex;
  margin-top: 48rpx;
}
.orderDetail .footer .noteTile {
  font-size:24rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.orderDetail .footer textarea {
  width: 539rpx;
  height: 128rpx;
  border:1px solid rgba(186,186,186,1);
}
.orderDetail .footer .footerDes{
  font-size:24rpx;
  font-weight:400;
  color: #999999;
  text-align: right;
}

.payTitle {
  height:131rpx;
  font-size:32rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(51,51,51,1);
  line-height:131rpx;
  text-align: center;
}

.pay{
  height: 606rpx;
  background-color: #fff;
  padding: 44rpx 38rpx 0 38rpx;
}

.pay .supportWrap {
  height: 113rpx;
  border-bottom: 1rpx solid #E4E4E4;
}

.pay .support {
  font-size:28rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
}

.pay .noSupport {
  font-size:22rpx;
  font-weight:300;
  color:rgba(153,153,153,1);
  margin-top: 10rpx;
}

.pay .wxConfirm {
  display: flex;
  height: 178rpx;
  align-items: center;
  border-bottom: 1px solid #E4E4E4;
}

.pay .wxConfirm .logo{
  width: 96rpx;
  height: 79rpx;
  margin-left: 21rpx;
}

.pay .wxConfirm .wxDes {
  margin-left: 79rpx;
}

.pay .wxConfirm .first {
  font-size:32rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
  height: 50rpx;
}

.pay .wxConfirm .second {
  font-size:24rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
  height: 50rpx;
}

.pay .wxConfirm .circle{
  margin-left: 260rpx;
  width: 39rpx;
  height: 39rpx;
  border: 1rpx solid #E4E4E4;
  border-radius: 50%;
  overflow: hidden;
}

.pay .wxConfirm .circle image {
   width: 39rpx;
   height: 39rpx;
}

.money {
  margin-top: 36rpx;
  overflow: hidden;
}

.money .centent {
  float: right;
}

.money .centent .ul{
  display: flex;
  justify-content: space-between;
  margin-top: 11rpx;
  margin-bottom: 11rpx;
}

.money .centent .ul .title{
  font-size:24rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
  margin-right: 57rpx;
}

.money .centent .ul .text{
  font-size:32rpx;
  font-weight:normal;
  color:rgba(102,102,102,1);
}

.money .centent .ul .total{
  font-size:28rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
}

.money .centent .ul .totalMoney{
  font-size:38rpx;
  font-weight:normal;
  color:rgba(0,0,0,1);
}

.foot{
  margin-top: 52rpx;
  height: 100rpx;
  display: flex;
}

.foot .left {
  width:518rpx;
  height: 100rpx;
  background:rgba(71,71,71,1);
  padding-left: 39rpx;
  box-sizing: border-box;
}

.foot .right {
  width:232rpx;
  height: 100rpx;
  background:rgba(192,158,87,1);
  font-size:30rpx;
  font-weight:400;
  color:rgba(37,37,34,1);
  line-height:100rpx;
  text-align: center;
}

.foot .left .computWrap{
  display: flex;
  align-items: center;
}

.foot .left .comput{
  font-size:32rpx;
  font-weight:300;
  color: #FFFFFF;
}

.foot .left .computMoney{
  font-size:40rpx;
  font-weight:normal;
  color:rgba(192,158,87,1);
}

.foot .left .Number{
  margin-top: 10rpx;
  font-size:20rpx;
  font-weight:300;
  color:rgba(202,202,202,1);
}
